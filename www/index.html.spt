    <tr>
        <td><img src="{{ organization.img_src }}"><a href="/members/{{ organization.slug }}/">{{ organization.name }}</a></td>
        <td class="numeric">{{ organization.current_fair_share|intcomma }}</a></td>
        <td class="numeric">{{ organization.nprojects }}</a></td>
        <td class="numeric">{{ organization.nproducts }}</a></td>
        <td class="numeric">{{ organization.member_since }}</a></td>
    </tr>
    {% endif %}
    {% endfor %}
